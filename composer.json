{
    "name": "moell/mojito",
    "description": "This is a component of a rapid build system.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "moell",
            "email": "moell91@foxmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "smartins/passport-multiauth": "~3.0",
        "spatie/laravel-permission": "~2.12"
    },
    "require-dev": {
        "orchestra/testbench": "~3.5",
        "mockery/mockery": "^1.0",
        "orchestra/database": "^3.5",
        "doctrine/dbal": "^2.8"
    },
    "autoload": {
        "psr-4": {
            "Moell\\Mojito\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Moell\\Mojito\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Moell\\Mojito\\Providers\\MojitoServiceProvider"
            ]
        }
    },
    "minimum-stability":"dev",
    "prefer-stable": true
}
